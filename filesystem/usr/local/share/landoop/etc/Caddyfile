    without /api/kafka-rest-proxy
}

proxy /api/kafka-connect 0.0.0.0:$CONNECT_PORT {
    without /api/kafka-connect
}

proxy /lenses 0.0.0.0:9991

browse /certs
browse /logs

header /coyote-tests/results Cache-Control "no-store"
